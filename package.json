{
  "name": "mip",
  "version": "0.0.1",
  "private": true,
  "description": "Landing page for mip",
  "main": "server/server.js",
  "scripts": {
    "build": "tsc --outDir ./server-dist --sourceMap ./server/server.ts",
    "start": "tsc --outDir ./server-dist --sourceMap ./server/server.ts && node ./server-dist/server.js",
    "postinstall": "webpack -p",
    "webpack-watch": "set NODE_ENV=developement && webpack-dev-server",
    "watch": "set NODE_ENV=developement && nodemon -e ts,js,html,ejs --exec ts-node -- ./server/server.ts"
  },
  "author": "Alexey Levin",
  "license": "ISC",
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.17",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1",
    "@types/rest": "^1.3.28",
    "@types/webpack": "^3.0.5",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "log4js": "^2.3.3",
    "observable": "^2.1.4",
    "react": "^15.6.1",
    "react-custom-scroll": "^2.0.1",
    "react-dom": "^15.6.1",
    "react-loader": "^2.4.2",
    "rest": "^2.0.0",
    "style-loader": "^0.18.2",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "velocity-animate": "^1.5.0",
    "webpack": "^3.4.1"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "nodemon": "^1.11.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.7",
    "tslint": "^5.5.0",
    "webpack-dev-server": "^2.6.1"
  }
}
